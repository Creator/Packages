local e="filesystem" local t=setmetatable({},{__index=getfenv()})t._G=t local a={} local function o(s) if a[s]then local h,r=loadstring(a[s],"["..e.."] "..s..".lua")if not h then error(r,0)end local d=setmetatable({},{__index=t}) setfenv(h,d)local l,u=pcall(h)if not l then error(u,0)end if u==nil then u={} for c,m in pairs(d)do u[c]=m end end t[s]=u return u else return error"file not found"end end local i={{name="sutils",path="sutils",url="https://raw.githubusercontent.com/awsumben13/Misc-Packages/master/build/sutils"}}local n=dofile".package/api.lua" for s=1,#i do if not n.exists(i[s].path)then if  not i[s].url or not n.install(i[s].path,i[s].url)then printError("Requires package "..i[s].name.." and cannot download.")return end end t[i[s].name]=n.load(i[s].path)end a.main="local e,t=fs,{} local function a(o)local i=sutils.split(o,\"/\") for n=#i,1,-1 do if i[n]==\"..\"then table.remove(i,n)table.remove(i,n-1)elseif i[n]==\".\"or i[n]==\"\"then table.remove(i,n)end end return table.concat(i,\"/\")end function t.create()local o={}o.mounts={}o.banned={}o.fsobj={} function o:resolvePath(n)n=n:lower()return a(n)end function o:getFilesystemAndPath(n)n=self:resolvePath(n)local s={fs=self,len=0,path=n} for h,r in pairs(self.mounts)do if#h>s.len and n:find(\"^\"..h)then s={fs=r,len=#h,path=n:sub(#h+1)}end end if s.fs~=self then return s.fs:getFilesystemAndPath(s.path)end for h,r in pairs(self.banned)do if n:find(\"^\"..h)then return false end end return self,s.path end function o:write(n,s)local r=self:open(n,\"w\") if r then r.write(s)r.close()end return false end function o:read(n) local s=self:open(n,\"r\")if s then local h=s.readAll()s.close()return h end end local i={\"list\",\"delete\",\"open\",\"exists\",\"isDir\",\"getFreeSpace\",\"isReadOnly\",\"makeDir\",\"getDrive\"}for n=1,#i do o[i[n]]=function(s,h,...)local t,h=s:getFilesystemAndPath(h)if t then return t.fsobj[i[n]](h,...)end end end function o:getSize(n,s) local t,n=self:getFilesystemAndPath(n) if t then if s and t.fsobj.isDir(n)then local h,r=0,t.fsobj.list(n) for d=1,#r do h=h+t:getSize(n..\"/\".. r[d],s)end return h end return t.fsobj.getSize(n)end return 0 end function o:copy(n,s) if self:isDir(n)then return error\"directories aren't currently supported\"end local t,h=self:getFilesystemAndPath(n) local r,s=self:getFilesystemAndPath(s) if t and r then local d=t:read(h)if d then r:write(s,d)return true end end end function o:move(n,s)return self:copy(n,s)and self:delete(n)end function o:combine(n,...)local s={...} for h=1,#s do n=n..\"/\"..tostring(s[h])end return n:gsub(\"//+\",\"/\")end function o:getDir(n)local n=self:resolvePath(n) if n:find\"/\"then return n:match\"^(.+)/\"else return\"\"end end function o:getName(n) local n=self:resolvePath(n)return n:gsub(\"^.+/\",\"\")end function o:getExtension(n) local n=self:resolvePath(n)local s=n:gsub(\"^.+/\",\"\") if s:find\"%.\"then return s:gsub(\".+%.\",\"\")end return\"\"end function o:find(n)return self.fsobj.find(n)end function o:mount(n,s)self.mounts[n]=s end function o:unmount(n)self.mounts[n]=nil end function o:ban(n) self.banned[self:resolvePath(n)]=true end function o:allow(n)self.banned[self:resolvePath(n)]= nil end function o:normalFS() local i={\"combine\",\"isReadOnly\",\"getSize\",\"move\",\"exists\",\"copy\",\"getFreeSpace\",\"makeDir\",\"find\",\"getDir\",\"delete\",\"open\",\"list\",\"getDrive\",\"getName\",\"isDir\"}local o={}for n=1,#i do o[i[n]]=function(...)return self[i[n]](self,...)end end return o end setmetatable(o,{__type=\"filesystem\"})return o end function t.createRedirect(o)o=a(o)local i={} local n={\"list\",\"delete\",\"open\",\"exists\",\"isDir\",\"getSize\",\"getFreeSpace\",\"isReadOnly\",\"makeDir\",\"getDrive\",\"find\"} for s=1,#n do i[n[s]]=function(h,...)if type(h)~=\"string\"then return error(\"expected string path, got \"..type(h))end h=a(h)return e[n[s]]( o..\"/\"..h,...)end end function i.copy(s,h)if type(s)~=\"string\"or type(h)~=\"string\"then return error\"Expected string, string\"end s,h=a(s,h)return e.copy( o..\"/\"..s,o..\"/\"..h)end function i.move(s,h)if type(s)~=\"string\"or type(h)~=\"string\"then return error\"Expected string, string\"end s,h=a(s,h)return e.move( o..\"/\"..s,o..\"/\"..h)end i.combine=e.combine i.getDir=e.getDir i.getName=e.getName return i end return t"return o"main"